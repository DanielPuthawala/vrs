<?xml version="1.0" ?>
<all>
	<key name="$schema" type="str">http://json-schema.org/draft-07/schema</key>
	<title type="str">GA4GH-VRS-Definitions</title>
	<type type="str">object</type>
	<definitions type="dict">
		<Variation type="dict">
			<description type="str">A representation of the state of one or more biomolecules.</description>
			<oneOf>
				<key name="$ref" type="str">#/definitions/MolecularVariation</key>
			</oneOf>
			<oneOf>
				<key name="$ref" type="str">#/definitions/SystemicVariation</key>
			</oneOf>
			<oneOf>
				<key name="$ref" type="str">#/definitions/UtilityVariation</key>
			</oneOf>
			<discriminator type="dict">
				<propertyName type="str">type</propertyName>
			</discriminator>
		</Variation>
		<MolecularVariation type="dict">
			<description type="str">A variation on a contiguous molecule.</description>
			<oneOf>
				<key name="$ref" type="str">#/definitions/Allele</key>
			</oneOf>
			<oneOf>
				<key name="$ref" type="str">#/definitions/Haplotype</key>
			</oneOf>
			<discriminator type="dict">
				<propertyName type="str">type</propertyName>
			</discriminator>
		</MolecularVariation>
		<UtilityVariation type="dict">
			<description type="str">A collection of Variation subclasses that cannot be constrained to a specific class of biological variation, but are necessary for some applications of VRS.</description>
			<oneOf>
				<key name="$ref" type="str">#/definitions/Text</key>
			</oneOf>
			<oneOf>
				<key name="$ref" type="str">#/definitions/VariationSet</key>
			</oneOf>
			<discriminator type="dict">
				<propertyName type="str">type</propertyName>
			</discriminator>
		</UtilityVariation>
		<SystemicVariation type="dict">
			<description type="str">A Variation of multiple molecules in the context of a system, e.g. a genome, sample, or homologous chromosomes.</description>
			<oneOf>
				<key name="$ref" type="str">#/definitions/CopyNumber</key>
			</oneOf>
			<oneOf>
				<key name="$ref" type="str">#/definitions/Genotype</key>
			</oneOf>
			<discriminator type="dict">
				<propertyName type="str">type</propertyName>
			</discriminator>
		</SystemicVariation>
		<Allele type="dict">
			<description type="str">The state of a molecule at a Location.</description>
			<additionalProperties type="bool">false</additionalProperties>
			<type type="str">object</type>
			<properties type="dict">
				<_id type="dict">
					<key name="$ref" type="str">#/definitions/CURIE</key>
					<description type="str">Variation Id. MUST be unique within document.</description>
				</_id>
				<type type="dict">
					<type type="str">string</type>
					<const type="str">Allele</const>
					<description type="str">MUST be &quot;Allele&quot;</description>
				</type>
				<location type="dict">
					<oneOf>
						<key name="$ref" type="str">#/definitions/CURIE</key>
					</oneOf>
					<oneOf>
						<key name="$ref" type="str">#/definitions/Location</key>
					</oneOf>
					<description type="str">Where Allele is located</description>
				</location>
				<state type="dict">
					<oneOf>
						<key name="$ref" type="str">#/definitions/SequenceState</key>
					</oneOf>
					<oneOf>
						<key name="$ref" type="str">#/definitions/SequenceExpression</key>
					</oneOf>
					<description type="str">An expression of the sequence state</description>
					<deprecated>
						<key name="$ref" type="str">#/definitions/SequenceState</key>
					</deprecated>
				</state>
			</properties>
			<item type="str">location</item>
			<item type="str">state</item>
			<item type="str">type</item>
		</Allele>
		<Haplotype type="dict">
			<description type="str">A set of non-overlapping Allele members that co-occur on the same molecule.</description>
			<additionalProperties type="bool">false</additionalProperties>
			<type type="str">object</type>
			<properties type="dict">
				<_id type="dict">
					<key name="$ref" type="str">#/definitions/CURIE</key>
					<description type="str">Variation Id. MUST be unique within document.</description>
				</_id>
				<type type="dict">
					<type type="str">string</type>
					<const type="str">Haplotype</const>
					<description type="str">MUST be &quot;Haplotype&quot;</description>
				</type>
				<members type="dict">
					<type type="str">array</type>
					<minItems type="int">2</minItems>
					<uniqueItems type="bool">true</uniqueItems>
					<items type="dict">
						<oneOf>
							<key name="$ref" type="str">#/definitions/Allele</key>
						</oneOf>
						<oneOf>
							<key name="$ref" type="str">#/definitions/CURIE</key>
						</oneOf>
					</items>
					<description type="str">List of Alleles, or references to Alleles, that comprise this Haplotype.</description>
				</members>
			</properties>
			<item type="str">members</item>
			<item type="str">type</item>
		</Haplotype>
		<Genotype type="dict">
			<description type="str">A set of trans-phased MolecularVariation members, with associated copy counts, across a specified number of genomic locus `copies`.</description>
			<additionalProperties type="bool">false</additionalProperties>
			<type type="str">object</type>
			<properties type="dict">
				<_id type="dict">
					<key name="$ref" type="str">#/definitions/CURIE</key>
					<description type="str">Variation Id. MUST be unique within document.</description>
				</_id>
				<type type="dict">
					<type type="str">string</type>
					<const type="str">Genotype</const>
					<description type="str">MUST be &quot;Genotype&quot;</description>
				</type>
				<members type="dict">
					<type type="str">array</type>
					<uniqueItems type="bool">true</uniqueItems>
					<minItems type="int">1</minItems>
					<items type="dict">
						<key name="$ref" type="str">#/definitions/GenotypeMember</key>
					</items>
					<description type="str">Each GenotypeMember in `members` describes a MolecularVariation and the count of that variation at the locus.</description>
				</members>
				<copies type="dict">
					<oneOf>
						<key name="$ref" type="str">#/definitions/Number</key>
					</oneOf>
					<oneOf>
						<key name="$ref" type="str">#/definitions/IndefiniteRange</key>
					</oneOf>
					<oneOf>
						<key name="$ref" type="str">#/definitions/DefiniteRange</key>
					</oneOf>
					<description type="str">The total number of copies of all MolecularVariation at this locus, MUST be greater than or equal to the sum of GenotypeMember copy counts. If greater than the total counts, this implies additional  MolecularVariation that are expected to exist but are not explicitly indicated.</description>
				</copies>
			</properties>
			<item type="str">copies</item>
			<item type="str">members</item>
			<item type="str">type</item>
		</Genotype>
		<Text type="dict">
			<description type="str">A free-text definition of variation.</description>
			<additionalProperties type="bool">false</additionalProperties>
			<type type="str">object</type>
			<properties type="dict">
				<_id type="dict">
					<key name="$ref" type="str">#/definitions/CURIE</key>
					<description type="str">Variation Id. MUST be unique within document.</description>
				</_id>
				<type type="dict">
					<type type="str">string</type>
					<const type="str">Text</const>
					<description type="str">MUST be &quot;Text&quot;</description>
				</type>
				<definition type="dict">
					<type type="str">string</type>
					<description type="str">A textual representation of variation not representable by other subclasses of Variation.</description>
				</definition>
			</properties>
			<item type="str">definition</item>
			<item type="str">type</item>
		</Text>
		<VariationSet type="dict">
			<description type="str">An unconstrained set of Variation members.</description>
			<type type="str">object</type>
			<additionalProperties type="bool">false</additionalProperties>
			<properties type="dict">
				<_id type="dict">
					<key name="$ref" type="str">#/definitions/CURIE</key>
					<description type="str">Variation Id. MUST be unique within document.</description>
				</_id>
				<type type="dict">
					<type type="str">string</type>
					<const type="str">VariationSet</const>
					<description type="str">MUST be &quot;VariationSet&quot;</description>
				</type>
				<members type="dict">
					<type type="str">array</type>
					<uniqueItems type="bool">true</uniqueItems>
					<items type="dict">
						<oneOf>
							<key name="$ref" type="str">#/definitions/CURIE</key>
						</oneOf>
						<oneOf>
							<key name="$ref" type="str">#/definitions/Variation</key>
						</oneOf>
					</items>
					<description type="str">List of Variation objects or identifiers. Attribute is required, but MAY be empty.</description>
				</members>
			</properties>
			<item type="str">members</item>
			<item type="str">type</item>
		</VariationSet>
		<CopyNumber type="dict">
			<additionalProperties type="bool">false</additionalProperties>
			<type type="str">object</type>
			<description type="str">The absolute count of discrete copies of a MolecularVariation, Feature, SequenceExpression, or a CURIE reference within a system (e.g. genome, cell, etc.).</description>
			<properties type="dict">
				<_id type="dict">
					<key name="$ref" type="str">#/definitions/CURIE</key>
					<description type="str">Variation Id. MUST be unique within document.</description>
				</_id>
				<type type="dict">
					<type type="str">string</type>
					<const type="str">CopyNumber</const>
					<description type="str">MUST be &quot;CopyNumber&quot;</description>
				</type>
				<subject type="dict">
					<oneOf>
						<key name="$ref" type="str">#/definitions/MolecularVariation</key>
					</oneOf>
					<oneOf>
						<key name="$ref" type="str">#/definitions/Feature</key>
					</oneOf>
					<oneOf>
						<key name="$ref" type="str">#/definitions/SequenceExpression</key>
					</oneOf>
					<oneOf>
						<key name="$ref" type="str">#/definitions/CURIE</key>
					</oneOf>
					<description type="str">Subject of the Copy Number object</description>
				</subject>
				<copies type="dict">
					<oneOf>
						<key name="$ref" type="str">#/definitions/Number</key>
					</oneOf>
					<oneOf>
						<key name="$ref" type="str">#/definitions/IndefiniteRange</key>
					</oneOf>
					<oneOf>
						<key name="$ref" type="str">#/definitions/DefiniteRange</key>
					</oneOf>
					<description type="str">The integral number of copies of the subject in a system</description>
				</copies>
			</properties>
			<allOf>
				<if type="dict">
					<properties type="dict">
						<copies type="dict">
							<key name="$ref" type="str">#/definitions/Number</key>
						</copies>
					</properties>
				</if>
				<then type="dict">
					<properties type="dict">
						<copies type="dict">
							<properties type="dict">
								<value type="dict">
									<minimum type="int">0</minimum>
								</value>
							</properties>
						</copies>
					</properties>
				</then>
			</allOf>
			<allOf>
				<if type="dict">
					<properties type="dict">
						<copies type="dict">
							<key name="$ref" type="str">#/definitions/IndefiniteRange</key>
						</copies>
					</properties>
				</if>
				<then type="dict">
					<properties type="dict">
						<copies type="dict">
							<properties type="dict">
								<value type="dict">
									<minimum type="int">0</minimum>
								</value>
							</properties>
						</copies>
					</properties>
				</then>
			</allOf>
			<allOf>
				<if type="dict">
					<properties type="dict">
						<copies type="dict">
							<key name="$ref" type="str">#/definitions/DefiniteRange</key>
						</copies>
					</properties>
				</if>
				<then type="dict">
					<properties type="dict">
						<copies type="dict">
							<properties type="dict">
								<min type="dict">
									<minimum type="int">0</minimum>
								</min>
								<max type="dict">
									<minimum type="int">0</minimum>
								</max>
							</properties>
						</copies>
					</properties>
				</then>
			</allOf>
			<item type="str">copies</item>
			<item type="str">subject</item>
			<item type="str">type</item>
		</CopyNumber>
		<Location type="dict">
			<description type="str">A contiguous segment of a biological sequence.</description>
			<oneOf>
				<key name="$ref" type="str">#/definitions/ChromosomeLocation</key>
			</oneOf>
			<oneOf>
				<key name="$ref" type="str">#/definitions/SequenceLocation</key>
			</oneOf>
			<discriminator type="dict">
				<propertyName type="str">type</propertyName>
			</discriminator>
		</Location>
		<ChromosomeLocation type="dict">
			<additionalProperties type="bool">false</additionalProperties>
			<description type="str">A Location on a chromosome defined by a species and chromosome name.</description>
			<type type="str">object</type>
			<properties type="dict">
				<_id type="dict">
					<key name="$ref" type="str">#/definitions/CURIE</key>
					<description type="str">Location Id. MUST be unique within document.</description>
				</_id>
				<type type="dict">
					<type type="str">string</type>
					<const type="str">ChromosomeLocation</const>
					<description type="str">MUST be &quot;ChromosomeLocation&quot;</description>
				</type>
				<species_id type="dict">
					<key name="$ref" type="str">#/definitions/CURIE</key>
					<default type="str">taxonomy:9606</default>
					<description type="str">CURIE representing a species from the [NCBI species taxonomy](https://registry.identifiers.org/registry/taxonomy). Default: &quot;taxonomy:9606&quot; (human)</description>
				</species_id>
				<chr type="dict">
					<type type="str">string</type>
					<description type="str">The symbolic chromosome name. For humans, For humans, chromosome names MUST be one of 1..22, X, Y (case-sensitive)</description>
				</chr>
				<interval type="dict">
					<key name="$ref" type="str">#/definitions/CytobandInterval</key>
					<description type="str">The chromosome region defined by a CytobandInterval</description>
				</interval>
			</properties>
			<item type="str">chr</item>
			<item type="str">interval</item>
			<item type="str">species_id</item>
			<item type="str">type</item>
		</ChromosomeLocation>
		<SequenceLocation type="dict">
			<additionalProperties type="bool">false</additionalProperties>
			<description type="str">A Location defined by an interval on a referenced Sequence.</description>
			<type type="str">object</type>
			<properties type="dict">
				<_id type="dict">
					<key name="$ref" type="str">#/definitions/CURIE</key>
					<description type="str">Location Id. MUST be unique within document.</description>
				</_id>
				<type type="dict">
					<type type="str">string</type>
					<const type="str">SequenceLocation</const>
					<description type="str">MUST be &quot;SequenceLocation&quot;</description>
				</type>
				<sequence_id type="dict">
					<key name="$ref" type="str">#/definitions/CURIE</key>
					<description type="str">A VRS Computed Identifier for the reference Sequence.</description>
				</sequence_id>
				<interval type="dict">
					<oneOf>
						<key name="$ref" type="str">#/definitions/SequenceInterval</key>
					</oneOf>
					<oneOf>
						<key name="$ref" type="str">#/definitions/SimpleInterval</key>
					</oneOf>
					<description type="str">Reference sequence region defined by a SequenceInterval.</description>
				</interval>
			</properties>
			<item type="str">interval</item>
			<item type="str">sequence_id</item>
			<item type="str">type</item>
		</SequenceLocation>
		<SequenceInterval type="dict">
			<description type="str">A SequenceInterval represents a span on a Sequence. Positions are always represented by contiguous spans using interbase coordinates or coordinate ranges.</description>
			<type type="str">object</type>
			<additionalProperties type="bool">false</additionalProperties>
			<properties type="dict">
				<type type="dict">
					<type type="str">string</type>
					<const type="str">SequenceInterval</const>
					<description type="str">MUST be &quot;SequenceInterval&quot;</description>
				</type>
				<start type="dict">
					<oneOf>
						<key name="$ref" type="str">#/definitions/Number</key>
					</oneOf>
					<oneOf>
						<key name="$ref" type="str">#/definitions/IndefiniteRange</key>
					</oneOf>
					<oneOf>
						<key name="$ref" type="str">#/definitions/DefiniteRange</key>
					</oneOf>
					<description type="str">The start coordinate or range of the interval. The minimum value of this coordinate or range is 0. MUST represent a coordinate or range less than the value of `end`.</description>
				</start>
				<end type="dict">
					<oneOf>
						<key name="$ref" type="str">#/definitions/Number</key>
					</oneOf>
					<oneOf>
						<key name="$ref" type="str">#/definitions/IndefiniteRange</key>
					</oneOf>
					<oneOf>
						<key name="$ref" type="str">#/definitions/DefiniteRange</key>
					</oneOf>
					<description type="str">The end coordinate or range of the interval. The minimum value of this coordinate or range is 0. MUST represent a coordinate or range greater than the value of `start`.</description>
				</end>
			</properties>
			<allOf>
				<if type="dict">
					<properties type="dict">
						<start type="dict">
							<key name="$ref" type="str">#/definitions/Number</key>
						</start>
					</properties>
				</if>
				<then type="dict">
					<properties type="dict">
						<start type="dict">
							<properties type="dict">
								<value type="dict">
									<minimum type="int">0</minimum>
								</value>
							</properties>
						</start>
					</properties>
				</then>
			</allOf>
			<allOf>
				<if type="dict">
					<properties type="dict">
						<start type="dict">
							<key name="$ref" type="str">#/definitions/IndefiniteRange</key>
						</start>
					</properties>
				</if>
				<then type="dict">
					<properties type="dict">
						<start type="dict">
							<properties type="dict">
								<value type="dict">
									<minimum type="int">0</minimum>
								</value>
							</properties>
						</start>
					</properties>
				</then>
			</allOf>
			<allOf>
				<if type="dict">
					<properties type="dict">
						<start type="dict">
							<key name="$ref" type="str">#/definitions/DefiniteRange</key>
						</start>
					</properties>
				</if>
				<then type="dict">
					<properties type="dict">
						<start type="dict">
							<properties type="dict">
								<min type="dict">
									<minimum type="int">0</minimum>
								</min>
								<max type="dict">
									<minimum type="int">0</minimum>
								</max>
							</properties>
						</start>
					</properties>
				</then>
			</allOf>
			<allOf>
				<if type="dict">
					<properties type="dict">
						<end type="dict">
							<key name="$ref" type="str">#/definitions/Number</key>
						</end>
					</properties>
				</if>
				<then type="dict">
					<properties type="dict">
						<end type="dict">
							<properties type="dict">
								<value type="dict">
									<minimum type="int">0</minimum>
								</value>
							</properties>
						</end>
					</properties>
				</then>
			</allOf>
			<allOf>
				<if type="dict">
					<properties type="dict">
						<end type="dict">
							<key name="$ref" type="str">#/definitions/IndefiniteRange</key>
						</end>
					</properties>
				</if>
				<then type="dict">
					<properties type="dict">
						<end type="dict">
							<properties type="dict">
								<value type="dict">
									<minimum type="int">0</minimum>
								</value>
							</properties>
						</end>
					</properties>
				</then>
			</allOf>
			<allOf>
				<if type="dict">
					<properties type="dict">
						<end type="dict">
							<key name="$ref" type="str">#/definitions/DefiniteRange</key>
						</end>
					</properties>
				</if>
				<then type="dict">
					<properties type="dict">
						<end type="dict">
							<properties type="dict">
								<min type="dict">
									<minimum type="int">0</minimum>
								</min>
								<max type="dict">
									<minimum type="int">0</minimum>
								</max>
							</properties>
						</end>
					</properties>
				</then>
			</allOf>
			<item type="str">end</item>
			<item type="str">start</item>
			<item type="str">type</item>
		</SequenceInterval>
		<CytobandInterval type="dict">
			<description type="str">A contiguous span on a chromosome defined by cytoband features. The span includes the constituent regions described by the start and end cytobands, as well as any intervening regions.</description>
			<type type="str">object</type>
			<additionalProperties type="bool">false</additionalProperties>
			<properties type="dict">
				<type type="dict">
					<type type="str">string</type>
					<const type="str">CytobandInterval</const>
					<description type="str">MUST be &quot;CytobandInterval&quot;</description>
				</type>
				<start type="dict">
					<key name="$ref" type="str">#/definitions/HumanCytoband</key>
					<description type="str">The start cytoband region. MUST specify a region nearer the terminal end (telomere) of the chromosome p-arm than `end`.</description>
				</start>
				<end type="dict">
					<key name="$ref" type="str">#/definitions/HumanCytoband</key>
					<description type="str">The start cytoband region. MUST specify a region nearer the terminal end (telomere) of the chromosome q-arm than `start`.</description>
				</end>
			</properties>
			<example type="dict">
				<type type="str">CytobandInterval</type>
				<start type="str">q22.2</start>
				<end type="str">q22.3</end>
			</example>
			<item type="str">end</item>
			<item type="str">start</item>
			<item type="str">type</item>
		</CytobandInterval>
		<SequenceExpression type="dict">
			<description type="str">An expression describing a Sequence.</description>
			<oneOf>
				<key name="$ref" type="str">#/definitions/LiteralSequenceExpression</key>
			</oneOf>
			<oneOf>
				<key name="$ref" type="str">#/definitions/DerivedSequenceExpression</key>
			</oneOf>
			<oneOf>
				<key name="$ref" type="str">#/definitions/RepeatedSequenceExpression</key>
			</oneOf>
			<discriminator type="dict">
				<propertyName type="str">type</propertyName>
			</discriminator>
		</SequenceExpression>
		<LiteralSequenceExpression type="dict">
			<description type="str">An explicit expression of a Sequence.</description>
			<type type="str">object</type>
			<additionalProperties type="bool">false</additionalProperties>
			<properties type="dict">
				<type type="dict">
					<type type="str">string</type>
